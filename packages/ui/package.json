{
  "name": "@repo/ui",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./shadcn/*": {
      "types": "./dist/components/shadcn/*.d.ts",
      "import": "./dist/components/shadcn/*.mjs",
      "require": "./dist/components/shadcn/*.cjs",
      "default": "./dist/components/shadcn/*.mjs"
    },
    "./magicui/*": {
      "types": "./dist/components/magicui/*.d.ts",
      "import": "./dist/components/magicui/*.mjs",
      "require": "./dist/components/magicui/*.cjs",
      "default": "./dist/components/magicui/*.mjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.cjs",
      "default": "./dist/lib/*.mjs"
    }
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": ">=1.3.3",
    "@radix-ui/react-dialog": ">=1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": ">=2.1.7",
    "@radix-ui/react-select": ">=2.2.6",
    "@radix-ui/react-separator": ">=1.1.7",
    "@radix-ui/react-slot": ">=1.2.3",
    "class-variance-authority": ">=0.7.1",
    "clsx": ">=2.1.1",
    "framer-motion": ">=12.0.0",
    "lucide-react": ">=0.544.0",
    "react": ">=18",
    "react-dom": ">=18",
    "tailwind-merge": ">=2.6.0",
    "tailwindcss": ">=4.0.0"
  },
  "devDependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": ">=12.0.0",
    "lucide-react": "^0.544.0",
    "tailwind-merge": "^2.6.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "motion": "^12.23.22"
  }
}
